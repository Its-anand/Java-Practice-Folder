package com.run;
class Con1 {
    private int a=10;
    public void show(){
    	System.out.println(a);
    }
}
public class Main {
public Main() {
	System.out.println("hwllo");
}
public static void main(String[] args) {
    Main mainObj = new Main(); // Create an instance of Main
    Con1 obj = new Con1();
    obj.show();
    Main.main(args);
}

}

